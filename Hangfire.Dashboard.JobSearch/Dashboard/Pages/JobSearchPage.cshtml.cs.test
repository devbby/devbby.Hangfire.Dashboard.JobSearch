using System.IO;
using Hangfire.Dashboard.Pages;

namespace Hangfire.Dashboard.JobSearch.Dashboard.Pages
{
    public partial class JobSearchPage : RazorPage
    {
        public override void Execute()
        {
            Layout = new LayoutPage("Job Search");
            var stream = this.GetType().Assembly
                .GetManifestResourceStream("Hangfire.Dashboard.JobSearch.Dashboard.Pages.JobSearch.html");
            
            View()
            
            var html = new StreamReader(stream).ReadToEnd();
            Write(Html.Raw(html)); 
        }
    }
}